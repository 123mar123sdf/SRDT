<Window x:Class="Расписание_занятий.Расписание1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
          
        xmlns:local="clr-namespace:Расписание_занятий"
        mc:Ignorable="d"
        Title="Расписание"   WindowStartupLocation="CenterScreen" Height="740" Width="1380">
    <Window.Background>
        <ImageBrush ImageSource="Иконки\ФФон.png" />
    </Window.Background>
    <Grid >

        <Button  Style="{DynamicResource DefaultButtonStyle}" x:Name="Back" ToolTip="Вернуться назад" Click="Back_Click"
Margin="1247,0,0,616" BorderBrush="{x:Null}" Background="{x:Null}">
            <Image Source="Иконки/Назад.png" Height="32" Width="32"></Image>
        </Button>

        <Frame Name="MainFrame"  NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1"  Navigated="MainFrame_Navigated"></Frame>

    </Grid>
</Window>


