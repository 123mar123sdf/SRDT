<Page x:Class="Расписание_занятий.Отчетыст"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Расписание_занятий"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Отчетыст" Height="532" Width="1372">
    <Page.Background>
        <ImageBrush ImageSource="Иконки/ФФон.png" />
    </Page.Background>

    <Grid>
        <Label Content="ОТЧЁТЫ" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="20" Margin="654,54,0,0" Background="{x:Null}" Foreground="#FFFF7956"/>
        <Button Style="{DynamicResource zz}"   Content="«Расписание»" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="498,269,0,0"   FontSize="12"  Height="46" FontFamily="Segoe UI"  Click="Button_Click_2" />
        <Button Style="{DynamicResource zz}"   Content="«Преподаватели»" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="498,171,0,0"   FontSize="12"  Height="48" FontFamily="Segoe UI"  Click="Button_Click_1" />
        <Button Style="{DynamicResource zz}"   Content="«Группы»" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="608,368,0,0"   FontSize="12"   Height="48" FontFamily="Segoe UI"  Click="Button_Click_5" />
        <Button Style="{DynamicResource zz}"   Content="«Предметы»" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="722,171,0,0"    FontSize="12"  Height="48" FontFamily="Segoe UI"  Click="Button_Click_4" />
        <Button Style="{DynamicResource zz}"   Content="«Кабинеты»" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="722,269,0,0"  FontSize="12"   Height="46" FontFamily="Segoe UI"  Click="Button_Click_3" />

    </Grid>
</Page>
